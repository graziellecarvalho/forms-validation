{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "ignoreDeprecations": "5.0",
    "strictNullChecks":false,
    "strictPropertyInitialization": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]    
}
